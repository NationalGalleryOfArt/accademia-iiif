                            "body": {
                                "type": "TextualBody",
                                "format": "text/html",
                                "value": "<p>Camerae (Camera di Roma)</p>"
                            }
                        },
                        {
                            "id": "https://nationalgalleryofart.github.io/accademia-iiif/documents/asrtncuff1115890129/canvas/1/annopage-2/anno-3",
